  "main": "src/index.ts",
  "scripts": {
    "start": "NODE_PATH='./node_modules' npm-run-all clean build --parallel watch:build watch:server --print-label",
    "build": "tsc; cp ./server/.env ./build/server",
    "build-project": "npm start; ",
    "clean": "rimraf ./build/server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=development npx nodemon server/src/index.ts",
    "lint": "eslint .  --ext .js",
    "lint-fix": "eslint --fix .  --ext .js",
    "watch:build": "tsc --watch",
    "watch:server": "nodemon './build/server/src/index.js' --watch './build/server'",
    "prepare": "husky install .husky"
  },
  "repository": {
    "type": "git",
